	<!-- Authenticated 2 -->
	
	<bean name="ManuAuthenticated" class="acme.framework.entities.Authenticated">
		<property name="userAccount" ref="ManuUserAccount" />
	</bean>
	
	
	<bean name="ManuUserAccount" class="acme.framework.entities.UserAccount">
		<property name="username" value="Manuel" />
		<property name="password" value="Manuel" />
		<property name="enabled" value="true" />
		<property name="identity">
			<bean class="acme.datatypes.UserIdentity">
				<property name="name" value="Manuel" />
				<property name="surname" value="Rubio" />
				<property name="email" value="manuel@acme.com" />
			</bean>
		</property>
		<property name="roles">
			<list>
				<ref bean="ManuAuthenticated" />

			</list>
		</property>
	</bean>
	
	

	<!-- MessageThreads -->

	<bean name="MessageThread1" class="acme.entities.messages.MessageThread">
		<property name="title" value="Primer Hilo"/>
		<property name="moment" value="2019/11/12 19:59" />
		<property name="owner" ref="AlvaroAuthenticated"/>
	</bean>


	<!-- Messages -->

	<bean name="Message01" class="acme.entities.messages.Message">
		<property name="title" value="Primer mensaje" />
		<property name="moment" value="2019/09/09 10:30" />
		<property name="tags" value="DP" />
		<property name="body" value="Este es el cuerpo del mensaje" />
		<property name="messageThread" ref="MessageThread1"/>
	</bean>
	
	<!-- Participants -->
	
	<bean name="Participant1" class="acme.entities.messages.ParticipatesIn">
		<property name="participant" ref="ManuAuthenticated"/>
		<property name="thread" ref="MessageThread1"/>
	</bean>
	
	<bean name="Requisito1" class="acme.entities.requisito.Requisito">
		<property name="text" value="Requisito1" />
		<property name="moreInfo" value="http://www.example.com" />
		<property name="job" ref="job1" />
		
	</bean>


</beans>
